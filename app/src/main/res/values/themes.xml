<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MedOk" parent="Theme.AppCompat.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/bar</item>
        <item name="colorPrimaryVariant">@color/bar</item>
        <item name="colorOnPrimary">@color/bar</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="drawerArrowStyle">@style/DrawerIcon</item>
    </style>

    <style name="DrawerIcon" parent="@style/Widget.AppCompat.DrawerArrowToggle">
        <item name="color">@android:color/white</item>
    </style>

    <style name="SpinnerStyle" parent="@android:style/Widget.TextView">
        <item name="android:textSize" >20sp</item>
        <item name="android:textColor" >"@color/EditTextAcc"</item>
    </style>

</resources>